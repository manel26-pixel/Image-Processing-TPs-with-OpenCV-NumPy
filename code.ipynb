    "''' TP 9 '''\n",
    "\n",
    "import cv2\n",
    "import numpy as np\n",
    "\n",
    "# Uncomment this line to use your phone's IP webcam\n",
    "# url = \"http://192.168.226.189:8080/video\"\n",
